t;\r\n    this.appointment = data.appointment;\r\n    let date = this.appointment.startDateTime.split(\" \");\r\n    this.date = date[0];\r\n    this.time = date[1];\r\n\r\n    this.cardLoader = false;\r\n  },\r\n\r\n  methods: {\r\n    fixDate(date) {\r\n      return moment(date).format(\"DD-MM-YYYY\");\r\n    },\r\n\r\n    editAppointment() {\r\n\r\n    },\r\n\r\n    ...mapActions({\r\n      getAppointment: \"appointment/getAppointment\",\r\n    }),\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"]}]}