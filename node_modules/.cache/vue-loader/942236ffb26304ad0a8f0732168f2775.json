v-list-item-content>\r\n            <v-list-item-title>Patients des services</v-list-item-title>\r\n          </v-list-item-content>\r\n        </v-list-item>\r\n        <!-- ========= My Affiliated Hospitals ========= -->\r\n        <!-- <v-list-item link disabled>\r\n          <v-list-item-action>\r\n            <v-icon>mdi-clipboard-list-outline</v-icon>\r\n          </v-list-item-action>\r\n          <v-list-item-content>\r\n            <v-list-item-title>Mon agenda</v-list-item-title>\r\n          </v-list-item-content>\r\n        </v-list-item> -->\r\n        <v-list-item link :to=\"`/hospital/statistics`\">\r\n          <v-list-item-action>\r\n            <v-icon>mdi-chart-bar</v-icon>\r\n          </v-list-item-action>\r\n          <v-list-item-content>\r\n            <v-list-item-title>Indicateurs statistiques</v-list-item-title>\r\n          </v-list-item-content>\r\n        </v-list-item>\r\n        <v-list-item link disabled>\r\n          <v-list-item-action>\r\n            <v-icon>mdi-calculator</v-icon>\r\n          </v-list-item-action>\r\n          <v-list-item-content>\r\n            <v-list-item-title>Comptabilité globale</v-list-item-title>\r\n          </v-list-item-content>\r\n        </v-list-item>\r\n        <!-- ========= General Settings ========= -->\r\n        <v-list-item link disabled>\r\n          <v-list-item-action>\r\n            <v-icon>mdi-account-cog-outline</v-icon>\r\n          </v-list-item-action>\r\n          <v-list-item-content>\r\n            <v-list-item-title>Paramètres généraux</v-list-item-title>\r\n          </v-list-item-content>\r\n        </v-list-item>\r\n        <v-list-item link disabled>\r\n          <v-list-item-action>\r\n            <v-icon>mdi-license</v-icon>\r\n          </v-list-item-action>\r\n          <v-list-item-content>\r\n            <v-list-item-title>Licence d'utilisation</v-list-item-title>\r\n          </v-list-item-content>\r\n        </v-list-item>\r\n      <!-- </v-list> -->\r\n\r\n      <v