\n        </div>\r\n      </v-card>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Login\",\r\n\r\n  data: () => ({\r\n    credentials: {\r\n      email: \"\",\r\n      password: \"\",\r\n    },\r\n    errorMsg: null,\r\n    loader: false,\r\n  }),\r\n\r\n  methods: {\r\n    async login() {\r\n      this.loader = true;\r\n\r\n      this.$store\r\n        .dispatch(\"auth/login\", this.credentials)\r\n        .then((data) => {\r\n          this.loader = false;\r\n          if(data.userType == 'hospitalHead') return this.$router.push('/hospital')\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n          this.loader = false;\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style></style>\r\n"],"sourceRoot":"src/views"}]}